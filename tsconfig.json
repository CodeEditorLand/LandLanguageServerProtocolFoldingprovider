{
	"compilerOptions": {
		"strictNullChecks": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": false,
		"inlineSources": false,
		"declaration": true,
		"stripInternal": true,
		"lib": [ "es2016" ],
		"outDir": "lib",
		"rootDir": "src"
	}
}